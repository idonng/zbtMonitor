  <!-- 根据关键词条集合获取分页的内容  -->
  <select id="getPublicOpinionBykeywords"  resultMap="BaseResultMap">
    select top ${pageSize}  
    <include refid="Base_Column_List" />
    from TB_PUBLIC_OPINION_INFO
    where Published_Date NOT IN (
		SELECT TOP (SELECT ${pageSize}*${pageNum-1}) Published_Date FROM TB_PUBLIC_OPINION_INFO 
			WHERE keyword in 
			<foreach item="item" index="index" collection="keywords" open="("
			separator="," close=")"> 
			#{item.keywordName}   
			</foreach>
			AND result_Effective = 1  
			<if test="resultType != 0">
		    AND result_Type = ${resultType}
		    </if>
		    <if test="begintime!=null and begintime!='' and endtime!=null and endtime!=''">	
			AND Published_Date between #{begintime} and #{endtime}
			</if>
			order by Published_Date desc
	 )
    AND keyword in 
    <foreach item="item" index="index" collection="keywords" open="("
			separator="," close=")"> 
	#{item.keywordName}   
	</foreach>	
    AND result_Effective = 1 
    <if test="resultType != 0">
    AND result_Type = ${resultType}
    </if>
    <if test="begintime!=null and begintime!='' and endtime!=null and endtime!=''">	
	AND Published_Date between #{begintime} and #{endtime}
	</if>
    order by Published_Date desc; 
  </select>
  
  
